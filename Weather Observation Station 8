SELECT DISTINCT CITY FROM STATION WHERE ( CITY LIKE 'A%' OR CITY LIKE 'E%' OR CITY LIKE 'I%' OR  CITY LIKE 'O%' OR CITY LIKE 'U%') AND ( CITY LIKE '%a' OR CITY LIKE '%e' OR CITY LIKE '%i' OR  CITY LIKE '%o' OR CITY LIKE '%u');
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

